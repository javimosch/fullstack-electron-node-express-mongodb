{
  "name": "fullstack-electron-node-express-mongodb",
  "version": "2.0.0",
  "license": "MIT",
  "build": {
    "asar": false,
    "appId": "com.misitioba.www.electron",
    "mac": {
      "category": "public.app-category.business",
      "icon": "src/resources/icon.icns",
      "type": "development",
      "publish": null
    },
    "files": [
      ".",
      "**/*",
      "./js/**",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!**/._*",
      "!.editorconfig",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.flowconfig,.yarn-metadata.json,.idea,.vs,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "./html/dist/js/*",
      "./js/**/*",
      "./js/fuse/*",
      "./js/fuse/singletons/*",
      "./js/fuse/singletons/**"
    ],
    "deb": {},
    "compression": "store",
    "productName": "electron-express-mongodb"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  },
  "dependencies": {
    "air-datepicker": "^2.1.0",
    "angular": "1.6.5",
    "angular-sanitize": "1.6.5",
    "bcrypt-nodejs": "0.0.3",
    "blob-util": "^1.3.0",
    "console-redirect": "^1.0.0",
    "create-if-not-exist": "^0.0.2",
    "debug": "^3.0.1",
    "dom-to-image": "^2.5.2",
    "dotenv": "^4.0.0",
    "electron-json-storage": "^2.0.0",
    "electron-pdf": "^1.2.2",
    "electron-pdf-window": "^1.0.12",
    "electron-storage": "^1.0.7",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "^2.0.0",
    "frontail": "^4.0.4",
    "geocomplete": "^1.7.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^4.3.11",
    "handlebars": "^4.0.10",
    "image-to-blob": "^0.2.0",
    "inquirer": "^4.0.0",
    "jquery": "^3.2.1",
    "keyboard-shortcut": "^1.2.0",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "minilog": "^3.1.0",
    "moment": "^2.18.1",
    "npm-run-all": "^4.1.1",
    "progress-bar": "^0.1.1",
    "raven": "^2.2.1",
    "read-dir-files": "^0.1.1",
    "rimraf-promise": "^2.0.0",
    "rivets": "^0.9.6",
    "sander": "^0.6.0",
    "scp2": "^0.5.0",
    "shell": "^0.3.2",
    "shelljs": "^0.7.8",
    "shortid": "^2.2.8",
    "ssh2": "^0.5.5",
    "stacktrace-js": "^2.0.0",
    "stream-to-blob": "^1.0.0",
    "uglify-js": "^3.0.28",
    "ui-select": "^0.19.8",
    "versiony": "^2.0.1",
    "watchjs": "^0.0.0",
    "web-frame": "^0.0.2",
    "yargs": "^9.0.1",
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "electron": "1.7.9",
    "electron-builder": "^19.42.1",
    "electron-webpack": "1.10.1",
    "webpack": "^3.8.1"
  }
}